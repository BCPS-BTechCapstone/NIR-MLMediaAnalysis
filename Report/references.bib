@article{dehkordiModelPredictiveControl2023,
  title = {Model Predictive Control of a Fermenter Using Dynamic Flux Balance Analysis Coupled with Convolutional Neural Networks},
  author = {Dehkordi, Milad Banitalebi and Pishvaie, Mahmoud Reza and Vafa, Ehsan},
  year = {2023},
