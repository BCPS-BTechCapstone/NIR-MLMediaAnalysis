  author = {Rstakyan, Viktorya and Mkhitaryan, Liana and Baghdasaryan, Lilit and Ghaltaghchyan, Tsovinar and Karabekian, Zaruhi and Sevoyan, Gohar and Aghayan, Marina and Rodr{\'i}guez, Miguel A.},
