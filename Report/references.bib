@article{shiDetectionBacterialPathogens2012,
  title = {Detection of Bacterial Pathogens in Aquaculture Samples by {{DNA}} Microarray Analysis},
  author = {Shi, Yu-Hong and Chen, Jiong and Li, Chang-Hong and Lu, Xin-Jiang and Zhang, De-Ming and Li, Hao-Yan and Zhao, Zhi-Xian and Li, Ming-Yun},
  year = {2012},
