@article{huangCrystallizationBehaviorPorous2014,
  title = {The Crystallization Behavior of Porous Poly(Lactic Acid) Prepared by Modified Solvent Casting/Particulate Leaching Technique for Potential Use of Tissue Engineering Scaffold},
  author = {Huang, Ran and Zhu, Xiaomin and Tu, Haiyan and Wan, Ajun},
