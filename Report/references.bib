  booktitle = {Comprehensive {{Medicinal Chemistry II}}},
  author = {Winiwarter, S. and Ridderstr{\"o}m, M. and Ungell, A. -L. and Andersson, T. B. and Zamora, I.},
  editor = {Taylor, John B. and Triggle, David J.},
  year = {2007},
  month = jan,
  pages = {531--554},
  publisher = {Elsevier},
  address = {Oxford},
  doi = {10.1016/B0-08-045044-X/00140-1},
  urldate = {2023-11-05},
  abstract = {The pharmacokinetic behavior of a drug has to be considered during the ADME properties are influenced by various factors. Different types of molecular descriptors are important, like lipophilicity and hydrogen bonding for the passive transcelullar permeability or pharmacophores or ligand--protein interaction for drug metabolism. Descriptors used for ADME modeling need to explain the relevant properties such as size, lipophilic and hydrophilic profiles, hydrogen bond patterns, pharmacophores, and ligand--protein interaction. In this chapter we give an overview on available descriptors for each of these properties and add information about various descriptor sets that have been used in ADME-modeling. We also discuss the importance of the knowledge about the property that is to be modeled in order to be able to interpret the results properly. Furthermore, the scope of the analysis is important: local models could use descriptors developed for a particular series of compounds. Moreover, these descriptors should be easy to interpret in order to suggest chemical changes in a molecule to obtain the desired ADME property or in the understanding of the complex of ADME data. Global models make use of descriptors that should be able to be computed easily for a large and diverse set of compounds, and they are mainly used in the drug discovery process to filter out compounds and select those with the appropriate properties.},
  isbn = {978-0-08-045044-5}
}

