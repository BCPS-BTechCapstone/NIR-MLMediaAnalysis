@article{zhangMeasuringCytotoxicityNew2007,
  title = {Measuring Cytotoxicity: A New Perspective on {{LC50}}},
  shorttitle = {Measuring Cytotoxicity},
  author = {Zhang, Ming and Aguilera, Dolly and Das, Chandra and Vasquez, Hernan and Zage, Peter and Gopalakrishnan, Vidya and Wolff, Johannes},
