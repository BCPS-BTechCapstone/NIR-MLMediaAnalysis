@article{lanepaixaodossantosEffectSugarConcentration2020,
  title = {Effect of Sugar Concentration ({$^\circ$}{{Brix}}) and Storage Temperature on the Time to Visible Growth of Individual Ascospores of Six Heat-Resistant Moulds Isolated from Fruit Products},
  author = {Lane Paix{\~a}o Dos Santos, Juliana and Samapundo, Simbarashe and Van Impe, Jan and Sant'Ana, Anderson S. and Devlieghere, Frank},
  year = {2020},
  month = feb,
  journal = {Food Control},
  volume = {108},
  pages = {106880},
  issn = {09567135},
  doi = {10.1016/j.foodcont.2019.106880},
  urldate = {2023-10-03},
  abstract = {Heat-resistant moulds (HRMs) pose great challenges to processors of fruit-based products due to their thermal resistance and ability to grow across a broad range of conditions. Therefore, the quantification of the effect of inhibitory factors (conditions) on the growth of HRMs is very important to be used to prevent spoilage during shelf-life. This study assessed the minimum temperature and maximum sugar content ({$^\circ$}Brix) for the growth of six HRMs (Byssochlamys spp. and Neosartorya spp.) previously isolated from fruit products. In addition, the time to form a visible colony (tv, days) was determined to assess biological variability of individual ascospores within same population. Heat activated ascospores (10 min at 80 {$^\circ$}C) were spread plated ( {\textpm} 100 spores) on acidified Potato Dextrose Agar (aPDA, pH 3.5) plates from which the {$^\circ$}Brix was adjusted with fructose-glucose (1:1) to levels between 44 and 59 {$^\circ$}Brix followed by incubation at 30 {$^\circ$}C. To assess the effect of temperature, inoculated plates of aPDA were incubated at 4, 7, 8, 10, 12 and 14 {$^\circ$}C. Three replicates (= 3 aPDA plates) were prepared per condition evaluated. The number of visible colonies were counted daily for up to two months. Probability distribution functions were then fitted in @Risk to the cumulative tv's. With regards to cold tolerance, B. nivea was the most cold sensitive as it had the least ability to germinate and form visible colonies at low temperatures (no growth when T {$\leq$} 10 {$^\circ$}C). On the other hand, N. hiratsukae was the most cold tolerant, being able to form visible colonies at temperatures {$\geq$} 7 {$^\circ$}C. Likewise, B. nivea was the most sensitive to increased {$^\circ$}Brix values, whilst N. udagawae was able to grow out at the highest {$^\circ$}Brix evaluated (59{$^\circ$}/aw = 0.86). The tolerance of potential spoilage HRMs to high sugar levels and their ability to grow under chilled conditions represent a challenge for the microbial stability of high-sugar fruit products. Differences in individual tv's were mostly observed under conditions at the growth/no growth regions. For instance, individual tv's of B. nivea ascospores ranged from 24 to 46 days at 12 {$^\circ$}C and those of N. udagawae ranged from 20 to 45 days at 59{$^\circ$}Brix. The tv's data from each HRM and condition evaluated were then fitted to different statistical distributions (Exponential, Normal, Lognormal, Weibull, Logistic or Pareto) to allow the use of the obtained data in further Quantitative Microbial Spoilage Risk Assessment work for pasteurized fruit products.},
  langid = {english}
}

