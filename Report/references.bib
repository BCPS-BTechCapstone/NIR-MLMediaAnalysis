@article{thapaUltimateProducerResponsibility2023,
  title = {Ultimate Producer Responsibility for E-Waste Management--{{A}} Proposal for Just Transition in the Circular Economy Based on the Case of Used {{European}} Electronic Equipment Exported to {{Nigeria}}},
  author = {Thapa, Kaustubh and Vermeulen, Walter J. V. and Deutz, Pauline and Olayide, Olawale},
