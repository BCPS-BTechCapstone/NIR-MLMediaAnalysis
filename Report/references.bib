@incollection{fischerDiagnosticTechniquesMicroarrays2008,
  title = {Diagnostic {{Techniques}}: {{Microarrays}}},
  shorttitle = {Diagnostic {{Techniques}}},
  booktitle = {Encyclopedia of {{Virology}} ({{Third Edition}})},
  author = {Fischer, K. and Urisman, A. and DeRisi, J.},
  editor = {Mahy, Brian W. J. and Van Regenmortel, Marc H. V.},
