@incollection{hayesChapter13Toxicologic2020,
  title = {Chapter 13 - {{Toxicologic}} Testing Methods},
  booktitle = {Loomis's {{Essentials}} of {{Toxicology}} ({{Fifth Edition}})},
  author = {Hayes, A. Wallace and Wang, Tao and Dixon, Darlene},
  editor = {Hayes, A. Wallace and Wang, Tao and Dixon, Darlene},
  year = {2020},
  month = jan,
  pages = {189--222},
  publisher = {Academic Press},
  doi = {10.1016/B978-0-12-815921-7.00013-2},
  urldate = {2024-03-24},
  abstract = {Toxicology testing includes general toxicology studies that are designed to evaluate the overall effects of compounds on experimental animals in various treatment durations (e.g., acute, subchronic, and chronic studies) and special studies that are designed to evaluate specific types of toxicity and their underlying mechanisms. Studies that will be used for regulatory purposes must be conducted in compliance of good laboratory practice (GLP). General toxicology studies include single-dose (acute) toxicity studies and repeat-dose toxicity studies. The purpose of reproductive and developmental toxicity studies is to evaluate compound-induced adverse effects on fertility, gestation, and development of offspring. Mutagenesis refers to alterations in the information content in the DNA sequence of an organism or a cell. Genotoxicity tests include the Ames test, the micronucleus assay (MN), the mouse lymphoma assay (MLA), the comet assay, sister chromatid exchange (SCE), and cytogenetic tests. Tests for carcinogenicity present one of the greatest challenges in toxicology, and there is an ongoing need to develop more robust tests in this area. There are also various specialized toxicity tests, including skin and eye irritation tests, cutaneous sensitization tests for immunotoxicity, phototoxicity tests, and eye tests. The nervous system (NS) is responsible for a wide array of biological functions including motor, sensory, autonomic, and cognitive capabilities. Chemicals that are neurotoxic can lead to a wide spectrum of changes in the structure, function, and development of the NS. Understanding mechanisms and effects of neurotoxicity thus requires significant knowledge of the central and peripheral nervous systems.},
  isbn = {978-0-12-815921-7},
  keywords = {Carcinogenicity,Developmental studies,Genotoxicity Neurotoxicity,Reproductive studies},
  file = {Y\:\\Documents\\Zotero\\Loomis's Essentials of Toxicology (Fifth Edition)\\Hayes et al_2020_Chapter 13 - Toxicologic testing methods.pdf;C\:\\Users\\corei\\Zotero\\storage\\PDSTMDNQ\\B9780128159217000132.html}
}

