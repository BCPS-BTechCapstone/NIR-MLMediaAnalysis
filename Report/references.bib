@article{rakovskyVTCPpolylactideCompositeScaffolds2014,
  title = {{$\beta$}-{{TCP-polylactide}} Composite Scaffolds with High Strength and Enhanced Permeability Prepared by a Modified Salt Leaching Method},
  author = {Rakovsky, Artoum and Gotman, Irena and Rabkin, Eugen and Gutmanas, Elazar Y.},
