@article{kesslerPracticalGuideTreatment2019,
  title = {A {{Practical Guide}} to {{Treatment}} of {{Childhood Absence Epilepsy}}},
  author = {Kessler, Sudha Kilaru and McGinnis, Emily},
  year = {2019},
  journal = {Paediatric Drugs},
  volume = {21},
  number = {1},
  pages = {15--24},
  issn = {1174-5878},
  doi = {10.1007/s40272-019-00325-x},
  urldate = {2023-12-05},
  abstract = {Childhood absence epilepsy (CAE) is a common pediatric epilepsy syndrome with distinct seizure semiology, electroencephalography (EEG) features, and treatment. A diagnosis of CAE can be obtained during an office visit with a careful history, physical exam including prolonged hyperventilation, and a routine EEG. The treatment of choice for CAE with absence seizures only is ethosuximide. Valproic acid and lamotrigine are also effective treatments for many patients, but when compared to ethosuximide, valproic acid has more adverse effects and lamotrigine is less effective. Attention to predictors of response to treatment, including clinical, electrographic, and genetic factors, is increasing. Refractory CAE occurs in fewer than half of patients, and treatment strategies are available, though efficacy data are lacking. Careful assessment and treatment of psychosocial comorbidities is essential in caring for patients with CAE.},
  pmcid = {PMC6394437},
  pmid = {30734897}
}

