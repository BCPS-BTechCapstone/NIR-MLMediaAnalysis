@article{yinRapidDetectionSalmonella2022,
  title = {Rapid Detection of {{Salmonella}} Enterica in Leafy Greens by a Novel {{DNA}} Microarray-Based {{PathogenDx}} System},
  author = {Yin, Hsin-Bai and Chen, Chi-Hung and Katchman, Benjamin and Newland, Cory and May, Melissa and Patel, Jitendra},
  year = {2022},
  month = oct,
  journal = {Food Microbiology},
  volume = {107},
  pages = {104086},
